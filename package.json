{
  "name": "nuxt-starter",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "nuxt typecheck",
    "test": "npm run test:all",
    "test:all": "npm run lint && npm run format:check && npm run typecheck"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.11.0",
    "@nuxt/icon": "^2.1.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxtjs/supabase": "^2.0.1",
    "nuxt": "^4.1.3",
    "vue": "^3.5.0",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,md}": [
      "prettier --write"
    ],
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
